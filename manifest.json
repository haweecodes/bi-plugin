{
    "name": "BI Reader",
    "version": "0.0.0.1",
    "description": "Lets you read prime contents.",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": ["declarativeContent", "storage"],
    "browser_action": {
      "default_icon": "./asset/bi.png",
      "default_popup": "popup.html"
    }
  }